        android:id="@+id/horizontalDivider"
        android:name="com.example.expo.HorizontalDivider"
        android:label="@string/menuHorizontalDivider"
        tools:layout="@layout/fragment_horizontaldivider" />
    <fragment
        android:id="@+id/searchView"
        android:name="com.example.expo.SearchView"
        android:label="@string/menuSearchView"
        tools:layout="@layout/fragment_searchview" />
    <fragment
        android:id="@+id/ratingbar"
        android:name="com.example.expo.Ratingbar"
        android:label="@string/menuRatingbar"
        tools:layout="@layout/fragment_ratingbar" />
</navigation>