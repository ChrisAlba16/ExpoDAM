<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/paginaInicio">

    <fragment
        android:id="@+id/paginaInicio"
        android:name="com.example.expo.PaginaInicio"
        android:label="@string/menuPaginaInicio"
        tools:layout="@layout/fragment_paginainicio" />

    <fragment
        android:id="@+id/videoView"
        android:name="com.example.expo.VideoView"
        android:label="@string/menuVideoView"
        tools:layout="@layout/fragment_videoview" />
    <fragment
        android:id="@+id/calendarView"
        android:name="com.example.expo.CalendarView"
        android:label="@string/menuCalendarView"
        tools:layout="@layout/fragment_calendarview" />
    <fragment
